{
  "name": "@nll/hkts",
  "version": "0.0.0",
  "description": "A tiny higher kinded type library",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "files": [
    "lib",
    "es6",
    "src"
  ],
  "scripts": {
    "build-es6": "tsc",
    "build-lib": "tsc --build tsconfig.lib.json",
    "build": "npm run build-es6 && npm run build-lib",
    "coveralls": "cat ./coverage/lcov.info | npx coveralls",
    "docs": "npx docs-ts",
    "jest-clear-cache": "jest --clearCache",
    "jest": "jest --ci",
    "prepublishOnly": "npm run build",
    "release": "npm run test && npm run build && npm run docs && npx standard-version",
    "test": "npm run jest-clear-cache && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullpub/hkts.git"
  },
  "keywords": [
    "hkts"
  ],
  "author": "Brandon Blaylock <brandon@null.pub> (https://blaylock.dev)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/nullpub/hkts/issues"
  },
  "homepage": "https://github.com/nullpub/hkts#readme",
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^12.7.2",
    "coveralls": "^3.1.0",
    "jest": "^26.4.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
